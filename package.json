{
  "name": "semantic-release-angular-npm",
  "description": "semantic-release plugin to compile and package Angular libraries in Angular Package Format (APF)",
  "version": "1.0.0",
  "author": "AngÃ©lica Flausino",
  "ava": {
    "files": [
      "test/**/*.test.js"
    ],
    "timeout": "2m",
    "workerThreads": false
  },
  "bugs": {
    "url": "https://github.com/angelicaflausino/semantic-release-angular-npm/issues"
  },
  "dependencies": {
    "@semantic-release/error": "^4.0.0",
    "aggregate-error": "^4.0.1",
    "execa": "^7.0.0",
    "fs-extra": "^11.0.0",
    "lodash-es": "^4.17.21",
    "nerf-dart": "^1.0.0",
    "ng-packagr": "^16.1.0",
    "normalize-url": "^8.0.0",
    "npm": "^9.5.0",
    "rc": "^1.2.8",
    "read-pkg": "^8.0.0",
    "registry-auth-token": "^5.0.0",
    "semver": "^7.1.2",
    "tempy": "^3.0.0"
  },
  "devDependencies": {
    "@semantic-release/git": "^10.0.1",
    "ava": "5.3.1",
    "c8": "8.0.0",
    "codecov": "3.8.3",
    "dockerode": "3.3.5",
    "got": "13.0.0",
    "p-retry": "5.1.2",
    "prettier": "3.0.0",
    "semantic-release": "21.0.7",
    "sinon": "15.2.0",
    "stream-buffers": "3.0.2"
  },
  "engines": {
    "node": ">=18"
  },
  "files": [
    "lib",
    "index.js"
  ],
  "homepage": "https://github.com/angelicaflausino/semantic-release-angular-npm#readme",
  "keywords": [
    "npm",
    "publish",
    "registry",
    "semantic-release",
    "version",
    "angular",
    "apf"
  ],
  "license": "MIT",
  "main": "index.js",
  "c8": {
    "include": [
      "lib/**/*.js",
      "index.js"
    ],
    "reporter": [
      "json",
      "text",
      "html"
    ],
    "all": true
  },
  "peerDependencies": {
    "semantic-release": ">=20.1.0"
  },
  "prettier": {
    "printWidth": 120,
    "trailingComma": "es5"
  },
  "publishConfig": {
    "access": "public",
    "provenance": true
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/angelicaflausino/semantic-release-angular-npm.git"
  },
  "scripts": {
    "codecov": "codecov -f coverage/coverage-final.json",
    "lint": "prettier --check \"{lib,test}/**/*.{js,json,ts}\" \"*.{md,json,js}\" \".github/**/*.yml\"",
    "lint:fix": "prettier --write \"{lib,test}/**/*.{js,json,ts}\" \"*.{md,json,js}\" \".github/**/*.yml\"",
    "pretest": "npm run lint",
    "test": "c8 ava -v",
    "test:ci": "c8 ava -v"
  },
  "type": "module",
  "renovate": {
    "extends": [
      "github>semantic-release/.github"
    ]
  }
}
